<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="safaya.ali.masbahati">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_android_circle"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_android_circle"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        android:versionCode="0.1"
        android:versionName="beta"
        <activity
            android:name=".CounterList"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="safaya.ali.masbahati.FloatingViewService"
            android:enabled="true"
            android:exported="false"/>

    </application>

</manifest>